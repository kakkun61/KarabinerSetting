<?xml version="1.0"?>
<root>
    
    <item>
        <name>F18 to Control_L</name>
        <identifier>private.f18_control</identifier>
        <autogen>__KeyToKey__ KeyCode::F18, KeyCode::CONTROL_L</autogen>
    </item>
    
    <item>
        <name>F19 to KANA</name>
        <identifier>private.f19_kana</identifier>
        <autogen>__KeyToKey__ KeyCode::F19, KeyCode::JIS_KANA</autogen>
    </item>
    
    <item>
        <name>Option_L to EISUU</name>
        <identifier>private.option_eisuu</identifier>
        <autogen>__KeyToKey__ KeyCode::OPTION_L, KeyCode::JIS_EISUU</autogen>
    </item>

    <item>
        <name>Mouse Button 4 & 5 to Browser Forward & Backward</name>
        <identifier>remap.mouse_button_4_5_forward_backward</identifier>
        <autogen>--PointingButtonToKey-- PointingButton::BUTTON4, KeyCode::JIS_BRACKET_LEFT,  ModifierFlag::COMMAND_L</autogen>
        <autogen>--PointingButtonToKey-- PointingButton::BUTTON5, KeyCode::JIS_BRACKET_RIGHT, ModifierFlag::COMMAND_L</autogen>
    </item>

    <item>
        <name>AquaSKK</name>
        <appdef>
            <appname>Terminal</appname>
            <equal>com.googlecode.iterm2</equal>
            <equal>com.apple.Terminal</equal>
        </appdef>
        <list>
            <item>
                 <name>Ctrl-J to Kana on Apple Terminal/iTerm2</name>
                 <only>Terminal</only>
                 <identifier>private.iterm2.ctrl_j_to_kana</identifier>
                 <autogen>
                     __KeyToKey__
                     KeyCode::J, ModifierFlag::CONTROL_L,
                     KeyCode::JIS_KANA
                 </autogen>
            </item>
            <item>
                 <name>Ctrl-J to Kana on Apple Terminal/iTerm2 for Dvorak user</name>
                 <only>Terminal</only>
                 <identifier>private.iterm2.ctrl_j_to_kana.dvorak</identifier>
                 <autogen>
                     __KeyToKey__
                     KeyCode::C, ModifierFlag::CONTROL_L,
                     KeyCode::JIS_KANA
                 </autogen>
            </item>
            <item>
                 <name>Ctrl-J to Kana on Apple Terminal/iTerm2 for Dvorak-like user</name>
                 <only>Terminal</only>
                 <identifier>private.iterm2.ctrl_j_to_kana.dvorak_like</identifier>
                 <autogen>
                     __KeyToKey__
                     KeyCode::P, ModifierFlag::CONTROL_L,
                     KeyCode::JIS_KANA
                 </autogen>
            </item>
        </list>
    </item>

    <item>
        <name>Dvorak</name>
        <list>
            <item>
                 <name>Qwerty to Dvorak-like layout</name>
                 <identifier>private.qwerty2dvorak</identifier>
                 <autogen> __KeyToKey__ KeyCode::Q,              KeyCode::JIS_ATMARK        </autogen>
                 <autogen> __KeyToKey__ KeyCode::W,              KeyCode::COMMA             </autogen>
                 <autogen> __KeyToKey__ KeyCode::E,              KeyCode::DOT               </autogen>
                 <autogen> __KeyToKey__ KeyCode::R,              KeyCode::V                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::T,              KeyCode::Y                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::Y,              KeyCode::F                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::U,              KeyCode::G                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::I,              KeyCode::K                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::O,              KeyCode::R                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::P,              KeyCode::J                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::BRACKET_LEFT,   KeyCode::JIS_BRACKET_LEFT  </autogen>
                 <autogen> __KeyToKey__ KeyCode::BRACKET_RIGHT,  KeyCode::JIS_BRACKET_RIGHT </autogen>

                 <autogen> __KeyToKey__ KeyCode::A,              KeyCode::A                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::S,              KeyCode::O                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::D,              KeyCode::E                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::F,              KeyCode::U                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::G,              KeyCode::I                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::H,              KeyCode::D                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::J,              KeyCode::H                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::K,              KeyCode::T                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::L,              KeyCode::N                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::SEMICOLON,      KeyCode::S                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::QUOTE,          KeyCode::SEMICOLON         </autogen>
                 <autogen> __KeyToKey__ KeyCode::BACKSLASH,      KeyCode::JIS_COLON         </autogen>

                 <autogen> __KeyToKey__ KeyCode::Z,
                                        ModifierFlag::SHIFT_L,   KeyCode::JIS_UNDERSCORE    </autogen>
                 <autogen> __KeyToKey__ KeyCode::Z,
                                        ModifierFlag::SHIFT_R,   KeyCode::JIS_UNDERSCORE    </autogen>
                 <autogen> __KeyToKey__ KeyCode::Z,              KeyCode::VK_JIS_BACKSLASH  </autogen>
                 <autogen> __KeyToKey__ KeyCode::X,              KeyCode::Q                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::C,              KeyCode::L                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::V,              KeyCode::C                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::B,              KeyCode::X                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::N,              KeyCode::B                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::M,              KeyCode::M                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::COMMA,          KeyCode::W                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::DOT,            KeyCode::P                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::SLASH,          KeyCode::Z                 </autogen>
                 <autogen> __KeyToKey__ KeyCode::JIS_UNDERSCORE, KeyCode::SLASH             </autogen>
            </item>
        </list>
    </item>
</root>
